{
  "name": "cupboard/core",
  "description": "The CupboardCMS core files",
  "keywords": [
    "blog",
      "laravel",
      "cms"
  ],
  "authors": [
    {
      "name": "Gary Rancier",
      "email": "gary.rancier@gmail.com",
      "role": "Developer"
    }
  ],
  "require": {
    "php": ">=5.4.0",
    "laravel/framework": "~4",
    "michelf/php-markdown": "1.6.*",
    "symfony/yaml": "2.8.*@dev",
    "intervention/image": "2.3.*"
  },
  "require-dev": {
    "mockery/mockery": "dev-master",
    "phpunit/phpunit": "5.4.*"
  },
  "autoload": {
    "psr-0": {
      "Cupboard\\Core": "src/",
      "Cupboard\\Database": "src/"
    }
  },
  "archive": {
    "exclude": [
      "assets"
    ]
  },
  "extra": {
      "branch-alias": {
          "dev-master": "1.1-dev"
      }
  },
  "minimum-stability": "dev",
  "license": "MIT"
}
